{
  "title": "IX.Retry",
  "version": "0.3.0-rc2",
  
  "buildOptions": {
    "compile": {
      "include": "*.cs"
    }
  },
  
  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-24027"
  },
  
  "frameworks": {
    "netstandard1.0":{
      "imports": [
        "wp81",
        "wp80"
      ]
    },
    "netstandard1.1":{
      "imports":[
        "net45",
        "win80"
      ]
    },
    "netstandard1.2":{
      "imports":[
        "net451",
        "net452",
        "win81",
        "wpa81"
      ]
    },
    "netstandard1.3":{
      "imports":[
        "net46"
      ]
    },
    "netstandard1.4":{
      "imports":[
        "net461",
        "uap10"
      ]
    },
    "netstandard1.5":{
      "imports":[
        "net462"
      ]
    },
    "netstandard1.6":{
      "imports":[
        "netcoreapp10",
        "net463"
      ]
    }
  },
  
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG"
        ]
      }
    },
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true,
        "optimize": true
      }
    },
    "Test": {
      "buildOptions": {
        "define": [
          "TEST"
        ]
      }
    }
  },
  
  "authors": [ "Adrian Mos" ],
  "copyright": "Adrian Mos, 2015",
  "description": "A library for re-queuing calls that might fail due to recoverable exceptions (such as network connection blips or remote servers too busy).",
  "packOptions": {
    "licenseUrl": "https://github.com/adimosh/IX.Retry/blob/master/LICENSE.md",
    "owners": [ "Adrian Mos" ],
    "projectUrl": "https://github.com/adimosh/IX.Retry/",
    "tags": [ "" ],
    "requireLicenseAcceptance": false
  }
}